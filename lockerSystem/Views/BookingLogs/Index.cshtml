@model IEnumerable<lockerSystem.Models.BookingLog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.bookBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.bookingStatues)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date_time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Booking_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.modifyBy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.bookBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.bookingStatues)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date_time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Booking_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modifyBy)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
