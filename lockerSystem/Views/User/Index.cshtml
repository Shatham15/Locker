 @model IEnumerable<tblUser> 
<div class="block mb-4">
    <div class="block-header border-bottom px-4 py-3">
        <h5 class="m-0">الطلبات</h5>
        <div class="d-flex gap-2">
            <div class="form-group">
                <input type="text" class="form-control table__quick_search"
                       placeholder="البحث السريع ...">
            </div>
            <a href="add_order.html" class="btn btn-primary">إضافة طلب</a>
        </div>
    </div>
    <div class="block-body">
        <table class="table table-hover js-dataTable">
            <thead>
                <tr>
                    <th> @Html.DisplayNameFor(m => m.fullName)</th>
                    <th> @Html.DisplayNameFor(m => m.email)</th>
                    <th> @Html.DisplayNameFor(m => m.userType)</th>
                    <th class=""></th>
                </tr>
            </thead>
            <tbody>
                @foreach(var x in Model){
                <tr class="cursor-pointer" >
                    <td>@x.fullName</td>
                    <td>@x.email</td>
                    <td>@x.userType</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
